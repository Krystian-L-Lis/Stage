    </Property>
    <Method Name="Set" Id="{de1e52bc-10d3-4d8a-8f2c-41becbc12b53}">
      <Declaration><![CDATA[(*Set each ARGB component value separately.*)
METHOD Set
VAR_INPUT
	nRed: USINT;
	nGreen: USINT;
	nBlue: USINT;
	nAlpha: USINT;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[_r := nRed;
_g := nGreen;
_b := nBlue;
_a := nAlpha;]]></ST>
      </Implementation>
    </Method>
