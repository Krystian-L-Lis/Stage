    <Property Name="Raw" Id="{7498ed64-bd4c-4ff5-82e1-2e5e41239c2d}">
      <Declaration><![CDATA[(*Compute single ARGB value from stored components when used as getter.
Sets ARGB values by extracting components from a single ARGB value.*)
PROPERTY Raw : Argb]]></Declaration>
      <Get Name="Get" Id="{7bd33e5f-4efa-455b-8c3e-295fca724c53}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[Raw := 	USINT_TO_UDINT(_b) OR
			SHL(USINT_TO_UDINT(_g), 8) OR
			SHL(USINT_TO_UDINT(_r), 16) OR
			SHL(USINT_TO_UDINT(_a), 24);]]></ST>
        </Implementation>
      </Get>
      <Set Name="Set" Id="{115bbdd4-d73f-4b8e-8f6c-e1130736bd89}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[_b := UDINT_TO_USINT(Raw AND 16#FF);
_g := UDINT_TO_USINT(SHR(Raw, 8) AND 16#FF);
_r := UDINT_TO_USINT(SHR(Raw, 16) AND 16#FF);
_a := UDINT_TO_USINT(SHR(Raw, 24) AND 16#FF);]]></ST>
        </Implementation>
      </Set>
    </Property>
