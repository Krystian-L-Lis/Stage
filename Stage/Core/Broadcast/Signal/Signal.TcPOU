    <Method Name="Emit" Id="{4ad2ae5f-8061-46ee-9938-a79c2993fbad}">
      <Declaration><![CDATA[METHOD Emit
VAR_INPUT
	iArg			: I_Arg := 0;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF iArg = 0 THEN
	iArg := _tag;
END_IF

_Emit(THIS^, iArg);]]></ST>
      </Implementation>
    </Method>
    <Method Name="EmitTo" Id="{01237bb5-eb33-4f6f-8303-4496429927bb}">
      <Declaration><![CDATA[METHOD EmitTo : Result
VAR_INPUT
	iReceiver		: I_Receiver;
	iArg			: I_Arg;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF iReceiver = 0 THEN
	EmitTo := Err.Itf0;
	RETURN;
END_IF

IF iArg = 0 THEN
	iArg := _tag;
END_IF

iReceiver.Receive(iArg);
EmitTo := Ok;
]]></ST>
      </Implementation>
    </Method>
    <Method Name="FB_init" Id="{ce47a828-df52-4ce9-b420-965840dc0bdc}">
      <Declaration><![CDATA[//FB_Init is always available implicitly and it is used primarily for initialization.
//The return value is not evaluated. For a specific influence, you can also declare the
//methods explicitly and provide additional code there with the standard initialization
//code. You can evaluate the return value.
METHOD FB_Init: BOOL
VAR_INPUT
    bInitRetains: BOOL; // TRUE: the retain variables are initialized (reset warm / reset cold)
    bInCopyCode: BOOL;  // TRUE: the instance will be copied to the copy code afterward (online change)   
END_VAR
VAR
	_eResult	: Result;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[CASE _RegSignal(THIS^) OF
	Err.Ovf:
		DebugMsg(THIS^, '_Broadcast.iSignal table overflow!');
END_CASE]]></ST>
      </Implementation>
    </Method>
    <Method Name="Option" Id="{1843f7ed-dc01-49ec-bf6f-06b77ee6ae5f}">
      <Declaration><![CDATA[METHOD Option : REFERENCE TO Signal
